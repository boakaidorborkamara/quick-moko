                            <div class="col-md-12 my-3">
                                <input type="checkbox" id="policy" name="Terms & Policy" value="True"  checked/>
                                <label for="policy">Accept Terms & Policy</label>
                            </div>
                            <div class="col-md-12">
                                <div class="submit_btn" id="registration-btn">
                                    <button class="boxed-btn3" type="submit">Register</button>
                                </div>
