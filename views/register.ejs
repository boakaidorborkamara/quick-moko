    <!-- apply_form_area -->
    <div class="apply_form_area">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12">
                    <form action="#" class="apply_form" id="registration-form">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="apply_info_text text-left">
                                    <h3>Personal Info</h3>
                                    <!-- <p>We provide online instant cash loans with quick approval that suit your term length</p> -->
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="First Name*" value="Boakai" required id="first-name">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Middle Name" value="Dobor" id="middle-name">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Last Name*" value="Kamara" required id="last-name">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Date of Birth*" value="2/3/2020" required id="date-of-birth">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Home Address*" value="Kebbah" required id="home-address">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Contact Number*" value="089393" required id="contact-number">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Position at Job*" value="Devloper" required id="position">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <select class="wide" id="salary-acceptance">
                                        <option data-display="Accept Salary Through Mobile Money*">Accept Salary Through Mobile Money</option>
                                        <option value="true" selected>Yes</option>
                                        <option value="false">No</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Mobile Money Number*" value="0889399393" required id="mobile-money-number">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="number" placeholder="Salary Per Month*" value="800" required id="salary-per-month">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <select class="wide" id="currency">
                                        <option data-display="Currency*">Currency</option>
                                        <option value="true">USD</option>
                                        <option value="false" selected>LRD</option>
                                      </select>
                                </div>
                            </div>
                            

                            <!-- employer's section  -->
                            <div class="col-lg-12 mt-5">
                                <div class="apply_info_text text-left">
                                    <h3>Employer Info</h3>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="single_field">
                                    <input type="text" placeholder="Employer's Name*" value="Easy Solar" required id="employer-name">
                                </div>
                            </div>
                            <div class="col-md-12">
